<div class="container padded">
  <div class="row">
    <div class="col-lg-6 col-lg-offset-3">
        <h2>Edit starting level</h2>

      <%= simple_form_for @student_metaclass do |f| %>
        <div class="form-inputs">
          <%= f.input :metaclassbeginlevel, autofocus: true %>
        </div>

        <div class="form-actions">
          <%= f.button :submit, "Edit level" %>
        </div>
      <% end %>
    </div>
  </div>
</div>

