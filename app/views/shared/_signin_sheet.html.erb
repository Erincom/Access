<%= form_tag(signin_sheet_path, :method => "get", class: 'inline-form') do %>
<%= select_tag :metaclass, options_from_collection_for_select(Metaclass.all.order('metaclasses.sessionid DESC'), :metaclassid, :metaclassname), { include_blank: false, class: 'select select_metaclassname' }  %>
<%= select_tag "month", options_for_select(1...12) %>
<!-- <div class="blank-space"></div> -->
<%= submit_tag "Preview", class: "btn btn-primary homepage-button" %>
<%= submit_tag "PDF", class: "btn btn-primary homepage-button" %>
<% end %>
